{
  "name": "liminoid-js",
  "version": "0.0.1",
  "description": "Embeddable client-side Python REPLs made possible through the magic of Web Assembly and Pyodide.",
  "main": "dist/bundle.js",
  "scripts": {
    "pretest": "rollup -c rollup.config.test.js && eslint src",
    "test": "npx serve",
    "test:dev": "rollup -c rollup.config.test.js --watch",
    "build": "rollup -c rollup.config.build.js",
    "postinstall": "mkdir dist/pyodide && wget -qO- https://github.com/iodide-project/pyodide/releases/download/0.14.3/pyodide-build-0.14.3.tar.bz2 | tar xvz - -C dist/pyodide"
  },
  "repository": {
    "url": "https://github.com/liminoid/liminoid-js",
    "type": "git"
  },
  "keywords": [
    "Javascript",
    "Web assembly",
    "Python",
    "Pyodide",
    "REPL"
  ],
  "author": "Jonathan Dinu",
  "license": "MIT",
  "homepage": "https://github.com/liminoid/liminoid-js",
  "bugs": "https://github.com/liminoid/liminoid-js/issues",
  "prettier": {
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/register": "^7.8.6",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-replace": "^2.3.1",
    "@rollup/plugin-sucrase": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "mocha": "^7.1.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-postcss": "^2.1.1"
  }
}
